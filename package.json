{
  "name": "cogniflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --base /CogniFlow/ && touch dist/.nojekyll",
    "preview": "vite preview",
    "deployToken": "dotenv -e .env.local -- gh-pages -d dist",
    "deploy": "gh-pages -d dist --repo git@github.com:CosmicWeave/CogniFlow.git",
    "fullDeploy": "npm install && npm run build && npm run deploy"
  },
  "dependencies": {
    "dexie": "4.0.8",
    "jszip": "3.10.1",
    "sql.js": "1.10.3",
    "canvas-confetti": "1.9.3",
    "zustand": "4.5.2",
    "@google/genai": "latest",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "dompurify": "3.1.2",
    "zod": "3.23.8",
    "papaparse": "5.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "dotenv-cli": "^10.0.0",
    "gh-pages": "^6.3.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}