{
  "name": "CogniFlow",
  "description": "An intelligent flashcard application using a spaced repetition system (SRS) to help you learn and retain information more effectively. Create your own decks, import from JSON, and study cards that are due for review.",
  "requestFramePermissions": [],
  "prompt": "",
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "application/zip": [
          ".apkg"
        ],
        "application/x-zip-compressed": [
          ".apkg"
        ],
        "application/octet-stream": [
          ".apkg"
        ],
        "application/json": [
          ".json"
        ],
        "text/csv": [
          ".csv"
        ],
        "image/png": [
          ".png"
        ],
        "image/jpeg": [
          ".jpg",
          ".jpeg"
        ],
        "image/webp": [
          ".webp"
        ],
        "image/gif": [
          ".gif"
        ]
      },
      "launch_type": "single-client"
    }
  ],
  "share_target": {
    "action": "/share-target/",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "file",
          "accept": [
            ".json",
            ".apkg",
            ".csv",
            "application/json",
            "application/zip",
            "application/x-zip-compressed",
            "application/octet-stream",
            "text/csv",
            "image/png",
            "image/jpeg",
            "image/webp",
            "image/gif"
          ]
        }
      ]
    }
  }
}